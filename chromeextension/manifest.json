{
  "manifest_version": 2,
  "name": "Copyraptor demo",
  "description": "Injects Copyraptor script into a page for demonstration.",
  "version": "1.0",

  "permissions": [ "activeTab" ],

  "background": {
    "scripts": ["contentscript.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Inject Copyraptor",
    "default_icon": "icon.png"
  }
}
